<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" layoutY="500.0" prefHeight="100.0" prefWidth="500.0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="500.0">
               <children>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="250.0" spacing="10.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#saveButtonAction" text="Save" />
                        <TextField fx:id="saveNameTF" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="250.0" spacing="10.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#loadButtonAction" text="Load" />
                        <ComboBox fx:id="mapFilesCombo" prefWidth="150.0" />
                     </children>
                  </HBox>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#statsButtonAction" text="Stats" />
                  <Button mnemonicParsing="false" onAction="#mainMenuButtonAction" text="Back to Main Menu" />
               </children>
            </HBox>
         </children></VBox>
      <ImageView fx:id="mapImage" fitHeight="500.0" fitWidth="800.0" onMouseClicked="#mapMouseClickedAction" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
